{
  "name": "glip-integration-github",
  "version": "0.10.3",
  "description": "glip-integration-github",
  "keywords": [
    "RingCentral",
    "source",
    "template",
    "Engage Digital",
    "SDK"
  ],
  "scripts": {
    "ngrok": "ngrok http -region ap 6066",
    "start": "node -r @babel/register -r dotenv/config ./src/server/run-server.js",
    "deploy": "npm run build-lambda && npm run deploy-lambda",
    "fix": "./node_modules/.bin/standard --fix",
    "compile": "babel src/server --out-dir dist/server",
    "c": "./node_modules/.bin/webpack-dev-server --progress --colors --config build/webpack.config.babel.js",
    "build": "npm run clean && npm run compile",
    "view": "node build/view.js",
    "clean": "node build/clean.js",
    "build-lambda": "npm run clean && node cmds/build && npm run view",
    "deploy-lambda": "node cmds/deploy",
    "watch": "cd deploy && ../node_modules/.bin/sls logs -f app -t",
    "lint": "./node_modules/.bin/standard",
    "update": "node -r dotenv/config cmds/update",
    "dynamo": "node -r @babel/register -r dotenv/config build/dynamo-local"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.4.0",
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.11.5",
    "antd": "^4.10.3",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-minify-webpack-plugin": "0.3.1",
    "babel-plugin-import": "1.13.0",
    "babel-plugin-lodash": "^3.3.4",
    "cheerio": "^1.0.0-rc.3",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "css-loader": "3.4.2",
    "download": "^8.0.0",
    "dynamodb-local": "0.0.31",
    "file-loader": "5.0.2",
    "json-deep-copy": "^1.1.6",
    "less": "3.11.1",
    "less-loader": "5.0.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "0.9.0",
    "ngrok": "^3.3.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "ringcentral-notification-integration-helper": "^0.4.2",
    "serverless": "^2.3.0",
    "serverless-deployment-bucket": "^1.3.0",
    "shelljs": "^0.8.4",
    "standard": "14.3.1",
    "stylus": "0.54.7",
    "stylus-loader": "3.0.2",
    "url-loader": "3.0.0",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-obfuscator": "^0.28.1",
    "yarn-upgrade-all": "^0.5.4"
  },
  "dependencies": {
    "@octokit/core": "^3.1.2",
    "@octokit/oauth-app": "^1.2.0",
    "@ringcentral/sdk": "^4.6.1",
    "dynamo-sequelize": "^0.8.1",
    "express-basic-auth": "^1.2.0",
    "express-jwt": "^6.0.0",
    "glip-integration-js": "^0.2.0",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.1.25",
    "pug": "^3.0.0",
    "ringcentral-chatbot-core": "^1.0.4",
    "sequelize": "^5.22.3",
    "serverless-http": "^2.6.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "*-compiled.js",
      "dist",
      "external"
    ]
  }
}
